<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="363dp"
        android:layout_height="528dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.497"
        app:srcCompat="@drawable/floor_selection_bg" />


    <ImageButton
        android:id="@+id/baseBtn"
        android:layout_width="230dp"
        android:layout_height="56dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.317"
        app:srcCompat="@drawable/btn_basement_floor" />


    <ImageButton
        android:id="@+id/groundBtn"
        android:layout_width="230dp"
        android:layout_height="56dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/baseBtn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/baseBtn"
        app:layout_constraintTop_toBottomOf="@+id/baseBtn"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/btn_ground_floor" />

    <ImageButton
        android:id="@+id/sec_btn"
        android:layout_width="230dp"
        android:layout_height="56dp"
        android:layout_marginBottom="124dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/groundBtn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/groundBtn"
        app:layout_constraintTop_toBottomOf="@+id/groundBtn"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/btn_second_floor" />

    <ImageButton
        android:id="@+id/fourth_btn"
        android:layout_width="230dp"
        android:layout_height="56dp"
        android:layout_marginBottom="52dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/third_btn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/third_btn"
        app:layout_constraintTop_toBottomOf="@+id/third_btn"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/btn_fouth_floor" />

    <ImageButton
        android:id="@+id/third_btn"
        android:layout_width="230dp"
        android:layout_height="56dp"
        android:layout_marginBottom="112dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/sec_btn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/sec_btn"
        app:layout_constraintTop_toBottomOf="@+id/sec_btn"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/btn_third_floor" />

    <ImageButton
        android:id="@+id/homepage_btn"
        android:layout_width="230dp"
        android:layout_height="56dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/baseBtn"
        app:layout_constraintEnd_toEndOf="@+id/baseBtn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/baseBtn"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/btn_homepage_floor" />

</androidx.constraintlayout.widget.ConstraintLayout>